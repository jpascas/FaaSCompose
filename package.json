{
  "name": "faascompose",
  "version": "0.1.0",
  "description": "Graphical User Interface for Composing FaaS Workflows",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/FaaSCompose.git"
  },
  "keywords": [
    "faas",
    "compose",
    "workflow",
    "serverless"
  ],
  "author": "FaaSCompose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oslabs-beta/FaaSCompose/issues"
  },
  "homepage": "https://github.com/oslabs-beta/FaaSCompose#readme",
  "dependencies": {
    "bootstrap": "^4.5.2",
    "nanoid": "^3.1.12",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "16.13.1",
    "react-flow-renderer": "^5.1.2"
  },
  "devDependencies": {
    "@testing-library/react": "^10.4.8",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  }
}
